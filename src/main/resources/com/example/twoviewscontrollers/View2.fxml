<VBox alignment="CENTER" prefHeight="308.0" prefWidth="527.0" spacing="20.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/19" fx:controller="com.example.twoviewscontrollers.Controller2">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>

    <Label fx:id="sayBye" text="Switch Back to Scene 1">
      <font>
         <Font size="20.0" />
      </font></Label>
    <Button fx:id="sayByeButton" onAction="#sayByeButtonPressed" prefHeight="26.0" prefWidth="199.0" text="Bye!" />
   <Button fx:id="scene1Button" onAction="#goBackToScene1" prefHeight="26.0" prefWidth="199.0" text="Go back to Scene 1" />
</VBox>
